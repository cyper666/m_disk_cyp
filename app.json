{
    "name": "mdisk userbot",
    "description": "mdisk uploader",
    "repository": "https://github.com/paavampayyan/mdisk.git",
    "keywords": [
        "python",
        "pyrogram"
    ],
    "image": "heroku/python",
    "formation": {
        "worker": {
            "quantity": 1,
            "size": "free"
        }
    },
    "env": {
        "API_ID": {
            "description": "App api_id from https://my.telegram.org"
        },
        "API_HASH": {
            "description": "App api_hash from https://my.telegram.org"
        },
        "SESSION": {
            "description": "pyrogram string"
        }
    }
}